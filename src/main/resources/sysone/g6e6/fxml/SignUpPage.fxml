<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="850.0" prefWidth="1500.0" stylesheets="@../styles/style.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sysone.g6e6.controller.SignUpController">
    <children>
        <Rectangle id="signupbox" arcHeight="5.0" arcWidth="5.0" fill="#667799" height="600.0" layoutX="475.0"
                   layoutY="131.0" stroke="WHITE" strokeType="INSIDE" width="550.0"/>
        <Text fill="WHITE" layoutX="482.0" layoutY="188.0" strokeType="OUTSIDE" strokeWidth="0.0"
              style="-fx-font-size: 30;" styleClass="label-custom-font" text="회원가입" textAlignment="CENTER"
              wrappingWidth="540.1999999999999">
            <font>
                <Font name="LG Smart UI Regular" size="17.0"/>
            </font>
        </Text>
        <Rectangle id="emailbox" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="525.0"
                   layoutY="524.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="341.0"/>
        <Rectangle id="pwcheckbox" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="525.0"
                   layoutY="447.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="450.0"/>
        <Rectangle id="pwbox" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="525.0" layoutY="368.0"
                   stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="450.0"/>
        <Rectangle id="idbox" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="525.0" layoutY="292.0"
                   stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="450.0"/>
        <Button id="inputbtn" layoutX="710.0" layoutY="648.0" mnemonicParsing="false" onAction="#handleSignUp"
                prefHeight="25.0" prefWidth="80.0" styleClass="label-custom-font" text=" 입력 ">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </Button>
        <Button id="checkbtn" layoutX="868.0" layoutY="297.0" mnemonicParsing="false" onAction="#handleDuplicationCheck"
                prefHeight="40.0" prefWidth="100.0" styleClass="label-custom-font" text="중복확인">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </Button>
        <TextField id="idtxt" fx:id="emailField" layoutX="529.0" layoutY="297.0" prefHeight="40.0" prefWidth="331.0"
                   promptText="아이디(이메일)" styleClass="label-custom-font">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </TextField>
        <PasswordField id="pwtxt" fx:id="passwordField" layoutX="531.0" layoutY="373.0" prefHeight="40.0"
                       prefWidth="437.0" promptText="비밀번호" styleClass="label-custom-font">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </PasswordField>
        <PasswordField id="pwchecktxt" fx:id="confirmPasswordField" layoutX="531.0" layoutY="451.0" prefHeight="40.0"
                       prefWidth="437.0" promptText="비밀번호 확인" styleClass="label-custom-font">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </PasswordField>
        <TextField id="emailtxt" fx:id="nicknameField" layoutX="530.0" layoutY="528.0" prefHeight="40.0"
                   prefWidth="331.0" promptText="닉네임" styleClass="label-custom-font">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </TextField>
        <Button id="checknickbtn" layoutX="876.0" layoutY="524.0" mnemonicParsing="false"
                onAction="#handleNicknameCheck" prefHeight="50.0" prefWidth="100.0" styleClass="label-custom-font"
                text="중복확인">
            <font>
                <Font name="LG Smart UI Light" size="12.0"/>
            </font>
        </Button>
        <Button layoutX="872.0" layoutY="169.0" mnemonicParsing="false"
                onAction="#handleReturnButton" prefHeight="37.0" prefWidth="100.0" styleClass="label-custom-font"
                text="돌아가기" textAlignment="CENTER"/>
    </children>
</AnchorPane>
