<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sysone.g6e6.controller.ReviewNoteChildController" prefHeight="100.0" prefWidth="500.0">
    <children>
        <VBox fx:id="parentVBox" prefWidth="500.0" AnchorPane.topAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0">
            <children>
                <AnchorPane fx:id="problemPane" style="-fx-background-color: #667799; -fx-background-radius: 10;" prefHeight="50.0" prefWidth="500.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <Label fx:id="problemLabel" alignment="TOP_LEFT" layoutX="20.0" layoutY="7.0" text="Label" wrapText="true" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" />
                    </children>
                </AnchorPane>
                <HBox fx:id="answerPane" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <Label fx:id="answerLabel" style="-fx-background-radius: 15;" text="Label" visible="false" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Button fx:id="answerScreenButton" mnemonicParsing="false" onAction="#HandleScreenButton" text="정답보기" textAlignment="CENTER" />
                        <Button mnemonicParsing="false" onAction="#handleErrorReport" text="신고하기" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
