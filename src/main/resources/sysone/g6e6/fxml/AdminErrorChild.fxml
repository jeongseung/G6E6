<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane minHeight="20.0" minWidth="520.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sysone.g6e6.controller.AdminErrorChildController">
    <HBox minHeight="20.0" minWidth="530.0" prefHeight="20.0" prefWidth="530.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <Label fx:id="nicknameLabel" alignment="CENTER" minHeight="20.0" minWidth="60.0"
               style="-fx-border-color: #000000;" text="닉네임">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
        <Label fx:id="emailLabel" alignment="CENTER" layoutX="10.0" layoutY="10.0" minHeight="20.0" minWidth="120.0"
               style="-fx-border-color: #000000;" text="이메일">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
        <Label fx:id="dateLabel" alignment="CENTER" layoutX="39.0" layoutY="10.0" minHeight="20.0" minWidth="70.0"
               style="-fx-border-color: #000000;" text="신고일시">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
        <Label fx:id="typeLabel" alignment="CENTER" layoutX="67.0" layoutY="10.0" minHeight="20.0" minWidth="90.0"
               style="-fx-border-color: #000000;" text="문제유형">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
        <Label fx:id="countLabel" alignment="CENTER" layoutX="96.0" layoutY="10.0" minHeight="20.0" minWidth="50.0"
               style="-fx-border-color: #000000;" text="신고횟수">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
        <Button fx:id="quizButton" onAction="#handleQuizButton" minHeight="20.0" minWidth="60.0"
                mnemonicParsing="false" text="문제">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Button fx:id="deleteButton" onAction="#handleDeleteButton" layoutX="540.0" layoutY="10.0" minHeight="20.0"
                minWidth="40.0"
                mnemonicParsing="false" text="삭제">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Button fx:id="keepButton" onAction="#handleKeepButton" layoutX="460.0" layoutY="10.0" minHeight="20.0"
                minWidth="40.0"
                mnemonicParsing="false" text="유지">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
    </HBox>
</AnchorPane>
