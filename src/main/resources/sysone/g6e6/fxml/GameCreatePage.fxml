<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="850.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sysone.g6e6.controller.GameCreateController">
    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f2f5fd" height="850.0" stroke="BLACK" strokeType="INSIDE"
                   strokeWidth="0.0" width="1500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        <Text layoutY="-1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="게임 생성" textAlignment="CENTER"
              wrappingWidth="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="60.0">
            <font>
                <Font name="System Bold" size="30.0"/>
            </font>
        </Text>
        <ChoiceBox fx:id="gameCreateChoiceBox1" layoutX="450.0" layoutY="165.0" prefHeight="50.0" prefWidth="600.0"
                   style="-fx-background-color: #FFFFFF; -fx-background-radius: 5;" AnchorPane.leftAnchor="450.0"
                   AnchorPane.rightAnchor="450.0" AnchorPane.topAnchor="200.0"/>
        <ChoiceBox fx:id="gameCreateChoiceBox2" layoutX="150.0" layoutY="200.0" prefHeight="50.0" prefWidth="600.0"
                   style="-fx-background-color: #FFFFFF; -fx-background-radius: 5;" AnchorPane.leftAnchor="450.0"
                   AnchorPane.rightAnchor="450.0" AnchorPane.topAnchor="400.0"/>
        <Button fx:id="gameCreateButton" layoutX="250.0" layoutY="345.0" mnemonicParsing="false" onAction="#startGame"
                prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 5;"
                text="입력" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="690.0" AnchorPane.rightAnchor="690.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Button fx:id="returnButton" layoutX="530.0" mnemonicParsing="false" onAction="#handleReturnButton"
                prefHeight="40.0" prefWidth="120.0" text="돌아가기" textAlignment="CENTER" AnchorPane.rightAnchor="40.0"
                AnchorPane.topAnchor="40.0"/>
    </children>
</AnchorPane>
